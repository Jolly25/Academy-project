		<provider>org.hibernate.jpa.HibernatePersistenceProvider</provider>
		
		<class>com.corso.model.Match</class>
		<properties>
			<property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/paesidb"/>
